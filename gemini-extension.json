{
  "name": "looker",
  "version": "0.1.3",
  "description": "Connect to Looker",
  "mcpServers": {
    "looker": {
      "command": "${extensionPath}${/}toolbox",
      "args": [
        "--prebuilt",
        "looker",
        "--stdio"
      ]
    }
  },
  "contextFileName": "LOOKER.md",
  "settings": [
    {
      "name": "Base URL",
      "description": "URL of the Looker instance",
      "envVar": "LOOKER_BASE_URL"
    },
    {
      "name": "Client ID",
      "description": "Client ID of the Looker API",
      "envVar": "LOOKER_CLIENT_ID"
    },
    {
      "name": "Client Secret",
      "description": "Client Secret of the Looker API",
      "envVar": "LOOKER_CLIENT_SECRET"
    },
    {
      "name": "Verify SSL",
      "description": "Whether to verify SSL certificates",
      "envVar": "LOOKER_VERIFY_SSL"
    },
    {
      "name": "Use Client OAuth",
      "description": "Whether to use OAuth for authentication",
      "envVar": "LOOKER_USE_CLIENT_OAUTH"
    },
    {
      "name": "Show Hidden Models",
      "description": "Whether to show hidden models",
      "envVar": "LOOKER_SHOW_HIDDEN_MODELS"
    },
    {
      "name": "Show Hidden Explores",
      "description": "Whether to show hidden explores",
      "envVar": "LOOKER_SHOW_HIDDEN_EXPLORES"
    },
    {
      "name": "Show Hidden Fields",
      "description": "Whether to show hidden fields",
      "envVar": "LOOKER_SHOW_HIDDEN_FIELDS"
    }
  ]
}